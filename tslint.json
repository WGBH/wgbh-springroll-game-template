{
    "rules": {
        "curly": true,
        "jsdoc-format": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-unused-expression": true,
        "no-var-keyword": true,
        "semicolon": [
            false,
            "always"
        ],
        "triple-equals": true
    }
}